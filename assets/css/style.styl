@import bootstrap

html
	overflow-y hidden

#log
	position fixed
	background #CCF
	font-size 130%
	font-family Consolas,Inconsolata,Courier New,monospace
	color #808080
	top 1px
	left 1px
	right 1px
	bottom 30px
	padding 10px 20px

#input
	position absolute
	left 1px
	right 27px
	bottom 1px

input
	width 100%
	padding-left 20px
	border 1px solid #999
	font-size 130%
	font-family Consolas,Inconsolata,Courier New,monospace
